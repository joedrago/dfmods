  </Binding>
  <Binding name="ARKINV_TOGGLE_AUCTION" category="BINDING_HEADER_ARKINV">
    ArkInventory.Frame_Main_Toggle( ArkInventory.Const.Location.Auction )
  </Binding>
  <Binding name="ARKINV_TOGGLE_EDIT" category="BINDING_HEADER_ARKINV">
    ArkInventory.Frame_Main_Show( ArkInventory.Const.Location.Bag )
    ArkInventory.ToggleEditMode( )
  </Binding>
  <Binding name="ARKINV_REFRESH" category="BINDING_HEADER_ARKINV">
    ArkInventory.Frame_Main_Generate( nil, ArkInventory.Const.Window.Draw.Recalculate )
  </Binding>
  <Binding name="ARKINV_RELOAD" category="BINDING_HEADER_ARKINV">
    ArkInventory.ItemCacheClear( )
