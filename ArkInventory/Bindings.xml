    ArkInventory.Frame_Rules_Toggle( )
  </Binding>
  <Binding name="ARKINV_TOGGLE_SEARCH" category="BINDING_HEADER_ARKINV">
    ArkInventory.Search.Frame_Toggle( )
  </Binding>
  <Binding name="ARKINV_LDB_PETS_SUMMON" category="BINDING_HEADER_ARKINV">
    ArkInventory.LDB.Pets.OnClick( )
  </Binding>
  <Binding name="CLICK ARKINV_MountToggle:LeftButton" category="BINDING_HEADER_ARKINV" />
  <Binding name="ARKINV_JUNK_SELL" category="BINDING_HEADER_ARKINV">
    ArkInventory.JunkSell( true )
  </Binding>
